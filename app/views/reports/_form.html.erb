<div class="section">
	<%= form_for @report do |f| %>

		<%= f.text_field :report_name %>
		<%= f.label :report_name %>

		<%= f.number_field :price %>
		<%= f.label :price %>

		<%= f.hidden_field :test_id %>
		<%= f.hidden_field :test_id_action %>

		<%= f.hidden_field :item_requirement_id %>
		<%= f.hidden_field :item_requirement_id_action %>

		<%= f.submit "Submit", :class => "waves-effect waves-light btn" %>

	<% end %>
	
</div>