<%= form_for @report, as: :report, method: @report.new_record? ? :post : :put do |f| %>
	
	<%= render :partial => "/shared/visibility", locals: {f: f} %>
		
	<% unless @report.new_record? %>
		<%= render :partial => "images/image", locals: {model: @report} %>
	<% end %>
	
	<%= build_nested_form(@report).html_safe %>	

	<%= f.submit "Submit" %>

<% end %>