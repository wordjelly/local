<%= form_for @credential, as: :credential do |f| %>
	
	<%= render :partial => "/shared/visibility", locals: {f: f} %>
		
	<% unless @credential.new_record? %>
		<%= render :partial => "images/image", locals: {model: @credential} %>
	<% end %>

	<%= build_nested_form(@credential).html_safe %>	

		
	<%= f.submit "Submit" %>

<% end %>